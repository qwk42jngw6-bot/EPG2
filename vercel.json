{
  "functions": {
    "api/now.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/now.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
